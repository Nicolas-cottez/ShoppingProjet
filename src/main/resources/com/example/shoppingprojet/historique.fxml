<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.shoppingprojet.Controleur.HistoriqueController"
      spacing="10">
    <TableView fx:id="tableHistorique">
        <columns>
            <TableColumn fx:id="colArticle" text="Article"/>
            <TableColumn fx:id="colIdCommande" text="ID"/>
            <TableColumn fx:id="colDate"       text="Date"/>
            <TableColumn fx:id="colHeure"      text="Heure"/>
            <TableColumn fx:id="colMontant"    text="Montant"/>
        </columns>
    </TableView>
</VBox>
